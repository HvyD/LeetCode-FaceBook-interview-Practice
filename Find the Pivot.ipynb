   "source": [
    "class Solution:\n",
    "    def pivotIndex(self, nums):\n",
    "        total = sum(nums)\n",
    "        left_sum = 0\n",
    "        for i, num in enumerate(nums):\n",
    "            if left_sum == (total-left_sum-num):\n",
    "                return i\n",
    "            left_sum += num\n",
    "        return -1"
   ]
